{
  "name": "FreeBoardGames.org",
  "version": "2.0.0",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "apollo:codegen": "rimraf src/gqlTypes && apollo client:codegen -c apollo.config.js --target typescript --outputFlat src/gqlTypes",
    "pre": "run-s lint format:check build:server test ci:test:e2e",
    "ci": "run-s lint format:check build:server ci:test:unit ci:test:e2e report:combined",
    "lint": "eslint --max-warnings=0 --ext .ts,.tsx src/",
    "lint:fix": "eslint --fix --max-warnings=0 --ext .ts,.tsx src/",
    "autopre": "run-s format lint:fix build:server test ci:test:e2e",
    "dev": "start-server-and-test dev:web http://localhost:3000 bgio:dev",
    "dev:web": "run-s build:server:dev dev:runserver",
    "dev:runserver": "node server/dist/server_web.js",
    "bgio:dev": "nodemon -e js,ts --exec yarn run bgio",
    "prod": "run-s build start",
    "build:server": "webpack --mode production --color --progress --config webpack.server.config.js",
    "build:server:dev": "webpack --mode development --color --progress --config webpack.server.config.js",
    "bgio": "run-s build:server start:bgio",
    "build": "cross-env BABEL_ENV=production NODE_ENV=production CHANNEL=production next build && yarn run build:storybook",
    "build:storybook": "cross-env BABEL_ENV=production NODE_ENV=production NODE_PATH=src/ build-storybook -o public/static/docs",
    "build:staging": "cross-env NODE_ENV=production CHANNEL=staging next build",
    "cyp:run:mobile": "cypress run --config video=false --env userAgent=mobile",
    "cyp:run:tablet": "cypress run --config video=false --env userAgent=tablet",
    "cyp:open:mobile": "cypress open --env userAgent=mobile",
    "cyp:open:tablet": "cypress open --env userAgent=tablet",
    "cyp:run": "run-s --silent cyp:run:*",
    "format": "prettier --write \"./**/*.*(js|jsx|ts|tsx)\"",
    "format:staged": "pretty-quick --staged",
    "format:check": "prettier --check \"./**/*.ts\" \"./**/*.tsx\" \"./**/*.js\"",
    "husky": "run-s lint format:staged test",
    "start:bgio": "node server/dist/server_bgio.js",
    "start": "run-s build:server start:server",
    "start:server": "cross-env CHANNEL=production NODE_ENV=production node server/dist/server_web.js",
    "e2e:build": "cross-env NODE_ENV=production CHANNEL=staging BABEL_ENV=e2e next build",
    "e2e:startserver": "cross-env NODE_ENV=production CHANNEL=staging BABEL_ENV=e2e node server/dist/server_web.js",
    "e2e:buildandstartserver": "run-s build:server e2e:build e2e:startserver",
    "ci:test:unit": "jest --coverage",
    "ci:test:e2e": "start-server-and-test e2e:buildandstartserver http://localhost:3000 cyp:run",
    "test": "jest",
    "test:coveralls": "cat coverage/lcov.info | node node_modules/.bin/coveralls",
    "mkdir:reports": "rimraf reports && mkdirp reports",
    "precopy:reports": "yarn run mkdir:reports",
    "copy:reports": "ncp coverage-unit/coverage-final.json reports/jest-unit.json && ncp coverage-e2e/coverage-final.json reports/jest-e2e.json",
    "precombine:reports": "yarn run copy:reports && mkdirp .nyc_output",
    "combine:reports": "nyc merge reports && move-cli coverage.json .nyc_output/out.json",
    "prereport:combined": "yarn run combine:reports",
    "report:combined": "nyc report --reporter lcov --report-dir coverage",
    "storybook": "cross-env NODE_PATH=src/ start-storybook -p 6006"
  },
