    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.1.3",
    "@nestjs/typeorm": "^7.1.0",
    "@types/shortid": "^0.0.29",
    "@types/superagent": "^4.1.7",
    "apollo": "^2.28.0",
    "apollo-server-express": "^2.14.2",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.2",
    "csurf": "^1.11.0",
    "express-winston": "^4.0.3",
