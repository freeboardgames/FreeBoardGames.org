{
  "name": "FreeBoardGame.org",
  "version": "2.0.0",
  "license": "BSD-3-Clause",
  "scripts": {
    "lint": "eslint --max-warnings=0 --ext .ts,.tsx pages src/ @types/ components/ tests/",
    "pre": "run-s lint format:check test:coverage test:cypress report:combined",
    "autopre": "run-s format lint test:coverage test:cypress report:combined",
    "dev": "node server.js",
    "prod": "run-s build start",
    "build:bgio": "webpack --color --progress --config webpack.bgio.config.js",
    "bgio": "run-s build:bgio run:bgio",
    "build": "cross-env BABEL_ENV=production NODE_ENV=production CHANNEL=production next build",
    "build:staging": "cross-env NODE_ENV=production CHANNEL=staging next build",
    "format": "prettier --write \"./**/*.*(js|jsx|ts|tsx)\"",
    "format:staged": "pretty-quick --staged",
    "format:check": "prettier --check \"./**/*.ts\" \"./**/*.tsx\" \"./**/*.js\" \"./**/*.jsx\"",
    "run:bgio": "node bgio/dist/server_bgio.js",
    "start": "cross-env CHANNEL=production NODE_ENV=production node server.js",
    "cyp:build": "cross-env NODE_ENV=production CHANNEL=staging BABEL_ENV=cypress next build",
    "cyp:startserver": "cross-env NODE_ENV=production CHANNEL=staging BABEL_ENV=cypress SERVER_PORT=13000 node server.js",
    "cyp:base": "cypress run --env type=base --config screenshotsFolder=cypress/snapshots/base",
    "cyp": "trash \"./cypress/snapshots/**/__diff_output__\" && yarn run cypress",
    "cyp:update": "trash \"./cypress/snapshots\" && yarn run cyp run --env updateSnapshots=true",
    "cyp:run": "cypress run --config video=false,baseUrl=http://localhost:13000",
    "cyp:buildandstartserver": "run-s cyp:build cyp:startserver",
    "test": "jest",
    "test:changed": "jest --onlyChanged",
    "test:changed:watch": "jest --watch --onlyChanged",
    "test:changed:coverage": "jest --coverage --onlyChanged",
    "test:changed:coverage:watch": "jest --coverage --onlyChanged --watch",
    "test:watch": "jest --watch",
    "test:watch:verbose": "jest --watch --verbose=true",
    "test:cypress": "start-server-and-test cyp:buildandstartserver http://localhost:13000 cyp:run",
    "test:coverage": "jest --coverage",
    "test:coveralls": "cat coverage/lcov.info | node node_modules/.bin/coveralls",
    "mkdir:reports": "mkdirp reports",
    "precopy:reports": "yarn run mkdir:reports",
    "copy:reports": "ncp coverage-cypress/coverage-final.json reports/from-cypress.json && ncp coverage-jest/coverage-final.json reports/from-jest.json",
    "precombine:reports": "yarn run copy:reports && mkdirp .nyc_output",
    "combine:reports": "nyc merge reports && move-cli coverage.json .nyc_output/out.json",
    "prereport:combined": "yarn run combine:reports",
    "report:combined": "nyc report --reporter lcov --report-dir coverage"
  },
  "dependencies": {
    "@cypress/webpack-preprocessor": "^4.1.1",
    "@freeboardgame.org/boardgame.io": "^0.0.12",
    "@material-ui/core": "^4.8.3",
    "@material-ui/icons": "^4.5.1",
    "@tweenjs/tween.js": "^18.5.0",
    "@types/socket.io-client": "^1.4.32",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-workers": "^1.0.0",
    "babel-plugin-jsx-remove-data-test-id": "^2.1.3",
    "chess.js": "^0.10.2",
    "copy-to-clipboard": "^3.2.0",
    "cross-env": "^6.0.3",
    "cypress": "^3.8.2",
    "empty-module": "^0.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-react": "^7.17.0",
    "mongodb": "3.0.3",
    "next": "9.1.8-canary.6",
    "next-offline": "^4.0.6",
    "next-seo": "^3.3.0",
    "next-with-error": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "qrcode.react": "^1.0.0",
    "raw-loader": "^4.0.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-ga": "^2.7.0",
    "react-markdown": "^4.3.1",
    "shuffle-array": "^1.0.1",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "superagent": "^5.1.3",
    "three": "^0.112.1",
    "webpackbar": "^4.0.0",
    "worker-loader": "^2.0.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^1.10.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.6",
    "@types/react": "^16.9.17",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "autoprefixer": "^9.7.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-preset-minify": "^0.5.1",
    "clean-css": "^4.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "coveralls": "^3.0.9",
    "cypress-image-snapshot": "^3.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "express": "^4.17.1",
    "hard-source-webpack-plugin": "^0.13.1",
    "ignore-loader": "^0.1.2",
    "image-trace-loader": "^1.0.2",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^7.1.0",
    "imagemin-svgo": "^7.0.0",
    "istanbul-lib-coverage": "^3.0.0",
    "jest": "^24.9.0",
    "jest-extended": "^0.11.2",
    "koa-no-cache": "^2.0.0",
    "lqip-loader": "^2.2.0",
    "mkdirp": "^0.5.1",
    "move-cli": "^1.2.1",
    "ncp": "^2.0.0",
    "next-optimized-images": "^2.5.4",
    "null-loader": "^3.0.0",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.0",
    "sharp": "^0.23.4",
    "start-server-and-test": "^1.10.6",
    "svg-sprite-loader": "^4.1.6",
    "trash-cli": "^3.0.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "webp-loader": "^0.6.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  },
  "nyc": {
    "report-dir": "coverage-cypress"
  }
}
